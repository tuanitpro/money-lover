{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "globals": {
    "localStorage": true,
    "fetch": true,
    "document": true,
    "window": true,
    "clearTimeout": true,
    "setTimeout": true
  }
}